{
  "bindings": [
    {
      "name": "blob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "input-images/{name}",
      "connection": "AZURE_STORAGE_CONN_STRING"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "path": "output-json/{name}.json",
      "connection": "AZURE_STORAGE_CONN_STRING"
    }
  ]
}
